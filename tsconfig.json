{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./src",
    "checkJs": false,
    "declaration": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": [
      "es2019",
      "dom",
      "esnext",
      "esnext.asynciterable"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "paths": {
      "@/*": [
        "*"
      ],
      "~/*": [
        "*"
      ]
    },
    "suppressImplicitAnyIndexErrors": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "target": "ESNext",
    "types": [
      "@types/node",
      "@nuxt/types",
      "@nuxtjs/sentry",
      "@nuxt/vue-app",
      "vue-router",
      "@inkline/inkline/types"
    ]
  }
}
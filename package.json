{
  "name": "zksync-web-portal",
  "description": "zkSync Portal",
  "private": true,
  "author": "Matter Labs",
  "homepage": "https://zksync.io",
  "keywords": [
    "zksync",
    "alpha",
    "dev-preview",
    "early-access",
    "zk-rollup",
    "frontend-team",
    "ethereum",
    "web3",
    "wallet",
    "defi",
    "typescript",
    "nuxt3",
    "tailwind",
    "tailwindui",
    "cryptopayments",
    "matter labs",
    "zksync dapps",
    "dapp",
    "ethers"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "file": "LICENSE-MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    },
    {
      "type": "Apache-2.0",
      "file": "LICENSE-APACHE",
      "url": "https://opensource.org/licenses/apache2.0.php"
    }
  ],
  "version": "0.1.0",
  "repository": "https://github.com/matter-labs/zksync-dapp-wallet-v2",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14.16.0 || ^16.11.0"
  },
  "scripts": {
    "nuxt-local": "sh ./cli-process-env.sh 'dev' 1 1",
    "ci:prepare:mainnet": "sh cli-process-env.sh 'mainnet' prod 1",
    "nuxt-ci": "sh ./cli-process-env.sh 'prod' 1 1",
    "nuxt-ci:build": "yarn run nuxt-ci && yarn run generate --fail-on-error",
    "generate": "yarn run nuxt-local && nuxt generate",
    "cspell": "cspell src/**/*.{md,vue} *.md",
    "lint:check": "eslint --ext .ts,.vue,.js \"src/**/*\"",
    "lint:fix": "eslint --ext .ts,.vue,.js --quiet --fix \"src/**/*\"",
    "format": "prettier --write \"src/**/*.{ts,vue,js,scss}\" --fix",
    "ci": "yarn run lint:check; yarn run cspell",
    "zk-ci-prepare": "yarn run nuxt-ci && yarn run generate --fail-on-error"
  },
  "bugs": {
    "url": "https://github.com/matter-labs/zksync-web-portal/issues"
  },
  "dependencies": {
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/sentry": "latest",
    "@types/js-cookie": "^2.2.6",
    "@types/lodash": "^4.14.168",
    "aos": "^2.3.4",
    "css-loader": "^5.0.2",
    "hammerjs": "^2.0.8",
    "nuxt": "latest",
    "nuxt-social-meta": "latest",
    "oh-vue-icons": "latest",
    "postcss": "7.0.36",
    "vue": "^2.6.12",
    "vue-loader": "^15.9.6",
    "vue-scrollto": "^2.20.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --format -E HUSKR",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "yarn format",
      "eslint --fix --"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-syntax-jsx": "latest",
    "@babel/preset-env": "7.12.17",
    "@inkline/nuxt": "^2.3.5",
    "@nuxt/types": "latest",
    "@nuxt/typescript-build": "^2.0.6",
    "@nuxtjs/composition-api": "^0.29.3",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "latest",
    "@types/aos": "^3.0.4",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/hammerjs": "^2.0.39",
    "@types/node-sass": "^4",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^4.15.1",
    "commitlint": "^11.0.0",
    "cspell": "^4.1.5",
    "dotenv": "8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "latest",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^7.6.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "node-sass": "4.14.1",
    "prettier": "^2.2.1",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^4.46.0"
  },
  "optionalDependencies": {
    "@sentry/webpack-plugin": "^1.14.1",
    "fs-extra": "^9.1.0"
  },
  "packageManager": "yarn@2.4.2"
}
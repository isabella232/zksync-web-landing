{
  "name": "zksync-web-portal",
  "description": "zkSync Portal",
  "private": true,
  "author": "Matter Labs",
  "homepage": "https://zksync.io",
  "keywords": [
    "zksync",
    "alpha",
    "dev-preview",
    "early-access",
    "zk-rollup",
    "frontend-team",
    "ethereum",
    "web3",
    "wallet",
    "defi",
    "typescript",
    "nuxt3",
    "tailwind",
    "tailwindui",
    "cryptopayments",
    "matter labs",
    "zksync dapps",
    "dapp",
    "ethers"
  ],
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "file": "LICENSE-MIT",
      "url": "https://www.opensource.org/licenses/mit-license.php"
    },
    {
      "type": "Apache-2.0",
      "file": "LICENSE-APACHE",
      "url": "https://opensource.org/licenses/apache2.0.php"
    }
  ],
  "version": "0.1.0",
  "repository": "https://github.com/matter-labs/zksync-dapp-wallet-v2",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "^14.16.0 || ^16.11.0"
  },
  "scripts": {
    "nuxt-local": "sh ./cli-process-env.sh 'dev' 1 1",
    "ci:prepare:mainnet": "sh cli-process-env.sh 'mainnet' prod 1",
    "nuxt-ci": "sh ./cli-process-env.sh 'prod' 1",
    "nuxt-ci:build": "yarn run nuxt-ci && yarn run generate --fail-on-error",
    "start:firebase": "firebase serve --only hosting",
    "dev": "yarn run nuxt-local && nuxt",
    "generate": "yarn run nuxt-local && nuxt generate",
    "format": "prettier --write",
    "lint-ts": "eslint --fix \"**/*.{ts, js, vue}\"",
    "lint-css": "stylelint --fix \"**/*.scss\"",
    "audit_dependencies": "yarn npm audit --package-lock-only --audit-level critical --json | yarn-audit-html --output audit.html",
    "fmt": "prettier --write \"docs/**/*.md\"",
    "fmt:check": "prettier --check \"docs/**/*.md\"",
    "cspell": "cspell src/**/*.{md,vue} *.md",
    "md:deadlinks": "check-md",
    "ci": "yarn run fmt:check; yarn run cspell; yarn run md:lint; yarn run md:deadlinks",
    "zk-ci-prepare": "yarn run nuxt-ci:build && yarn run generate --fail-on-error",
    "release": "shipjs prepare"
  },
  "check-md": {
    "cwd": "./docs",
    "defaultIndex": [
      "README.md"
    ],
    "exitLevel": "warn",
    "ignoreFootnotes": true
  },
  "bugs": {
    "url": "https://github.com/matter-labs/zksync-web-portal/issues"
  },
  "dependencies": {
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/google-gtag": "^1.0.4",
    "@nuxtjs/sentry": "^5.1.5",
    "aos": "^2.3.4",
    "hammerjs": "^2.0.8",
    "nuxt": "^2.15.8",
    "nuxt-social-meta": "^1.0.0",
    "oh-vue-icons": "^1.0.0-rc1",
    "postcss": "7.0.39",
    "vue": "^2.6.14",
    "vue-scrollto": "^2.20.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --format -E HUSKR",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,vue}": [
      "yarn format",
      "eslint --fix --"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.13.1",
    "@babel/plugin-syntax-jsx": "^7.16.0",
    "@babel/preset-env": "7.12.17",
    "@inkline/nuxt": "^2.3.6",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-build": "^2.1.0",
    "@nuxt/vue-app": "^2.15.8",
    "@nuxtjs/composition-api": "^0.30.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/style-resources": "^1.2.1",
    "@octokit/core": "^3.5.1",
    "@types/aos": "^3.0.4",
    "@types/babel__core": "^7.1.16",
    "@types/babel__preset-env": "^7.9.2",
    "@types/hammerjs": "^2.0.40",
    "@types/lodash": "^4.14.176",
    "@types/node-sass": "^4.11.2",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vue/babel-preset-app": "^4.5.15",
    "check-md": "^1.0.2",
    "commitlint": "^11.0.0",
    "consola": "^2.15.3",
    "cspell": "^5.12.6",
    "dotenv": "8.2.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^2.1.1",
    "eslint-plugin-vue": "^7.6.0",
    "fork-ts-checker-webpack-plugin": "^6.4.0",
    "node-sass": "^4.14.1",
    "prettier": "^2.4.1",
    "sass-loader": "10.2.0",
    "shipjs": "0.24.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-scss": "^3.21.0",
    "ts-loader": "8.3.0",
    "typescript": "^4.4.4",
    "vue-class-component": "^7.2.6",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "eslintConfig": {
    "extends": "@nuxtjs/eslint-config"
  },
  "optionalDependencies": {
    "@sentry/webpack-plugin": "^1.18.3",
    "fs-extra": "^10.0.0"
  },
  "packageManager": "yarn@2.4.2",
  "peerDependencies": {
    "webpack": "^5.64.0"
  }
}
